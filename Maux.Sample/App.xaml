<?xml version = "1.0" encoding = "UTF-8" ?>
<maux:MauxApplication xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:maux="clr-namespace:Maux;assembly=Maux.Mvvm"
             x:Class="Maux.Sample.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
                <ResourceDictionary Source="Resources/Styles/MaterialIcons.xaml" />
                <ResourceDictionary Source="Resources/Converters/Toolkit.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</maux:MauxApplication>
