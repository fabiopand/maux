    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:maux="clr-namespace:Maux;assembly=Maux.Mvvm">

    <ShellContent
        Title="Agenda"
        ContentTemplate="{maux:ShellContentTemplate}"
        Route="AgendaPage">
